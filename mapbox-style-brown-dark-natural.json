{
  "version": 8,
  "name": "Filter Brown Dark Natural",
  "sources": {
    "mapbox-streets": {
      "type": "vector",
      "url": "mapbox://mapbox.mapbox-streets-v8"
    }
  },
  "sprite": "mapbox://sprites/mapbox/dark-v11",
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#3D2E24"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "water",
      "paint": {
        "fill-color": "#264653"
      }
    },
    {
      "id": "landcover-grass",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "grass"],
      "paint": {
        "fill-color": "#2D4A3E",
        "fill-opacity": 0.7
      }
    },
    {
      "id": "landcover-wood",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landcover",
      "filter": ["==", ["get", "class"], "wood"],
      "paint": {
        "fill-color": "#2A4238",
        "fill-opacity": 0.7
      }
    },
    {
      "id": "national-park",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landuse_overlay",
      "filter": ["==", ["get", "class"], "national_park"],
      "paint": {
        "fill-color": "#2D4A3E",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landuse-park",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "park"],
      "paint": {
        "fill-color": "#2D4A3E",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "landuse-pitch",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "pitch"],
      "paint": {
        "fill-color": "#2D4A3E",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landuse-cemetery",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "cemetery"],
      "paint": {
        "fill-color": "#2A4238",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landuse-hospital",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "hospital"],
      "paint": {
        "fill-color": "#3D2E24",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landuse-school",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "school"],
      "paint": {
        "fill-color": "#3D2E24",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "landuse-industrial",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "industrial"],
      "paint": {
        "fill-color": "#332720",
        "fill-opacity": 0.5
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "building",
      "minzoom": 13,
      "paint": {
        "fill-color": "#4A3B30",
        "fill-opacity": [
          "interpolate", ["linear"], ["zoom"],
          13, 0,
          14, 0.6,
          16, 0.8
        ]
      }
    },
    {
      "id": "building-outline",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "line-color": "#2A1E17",
        "line-width": 0.5,
        "line-opacity": 0.4
      }
    },
    {
      "id": "admin-1-boundary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "admin",
      "filter": ["==", ["get", "admin_level"], 1],
      "paint": {
        "line-color": "#5C4A3D",
        "line-width": 0.75,
        "line-dasharray": [3, 2],
        "line-opacity": 0.6
      }
    },
    {
      "id": "admin-0-boundary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "admin",
      "filter": ["==", ["get", "admin_level"], 0],
      "paint": {
        "line-color": "#6B5548",
        "line-width": 1,
        "line-opacity": 0.8
      }
    },
    {
      "id": "tunnel-street",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["all", ["==", ["get", "structure"], "tunnel"], ["match", ["get", "class"], ["street", "street_limited"], true, false]],
      "paint": {
        "line-color": "#4A3B30",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          12, 0.5,
          16, 4
        ]
      }
    },
    {
      "id": "road-path",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["==", ["get", "class"], "path"],
      "paint": {
        "line-color": "#5C4A3D",
        "line-width": 1,
        "line-dasharray": [2, 1],
        "line-opacity": 0.5
      }
    },
    {
      "id": "road-street",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["all", ["!=", ["get", "structure"], "tunnel"], ["match", ["get", "class"], ["street", "street_limited"], true, false]],
      "paint": {
        "line-color": "#5C4A3D",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          12, 0.5,
          14, 2,
          16, 4
        ]
      }
    },
    {
      "id": "road-secondary-tertiary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["all", ["!=", ["get", "structure"], "tunnel"], ["match", ["get", "class"], ["secondary", "tertiary"], true, false]],
      "paint": {
        "line-color": "#6B5548",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          8, 0.5,
          14, 3,
          16, 6
        ]
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["all", ["!=", ["get", "structure"], "tunnel"], ["==", ["get", "class"], "primary"]],
      "paint": {
        "line-color": "#7A6355",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          6, 0.5,
          14, 4,
          16, 8
        ]
      }
    },
    {
      "id": "road-motorway-trunk",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["all", ["!=", ["get", "structure"], "tunnel"], ["match", ["get", "class"], ["motorway", "trunk"], true, false]],
      "paint": {
        "line-color": "#8B7355",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          5, 0.5,
          14, 5,
          16, 10
        ]
      }
    },
    {
      "id": "bridge-street",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["all", ["==", ["get", "structure"], "bridge"], ["match", ["get", "class"], ["street", "street_limited"], true, false]],
      "paint": {
        "line-color": "#5C4A3D",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          12, 0.5,
          14, 2,
          16, 4
        ]
      }
    },
    {
      "id": "bridge-primary",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["all", ["==", ["get", "structure"], "bridge"], ["==", ["get", "class"], "primary"]],
      "paint": {
        "line-color": "#7A6355",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          6, 0.5,
          14, 4,
          16, 8
        ]
      }
    },
    {
      "id": "bridge-motorway-trunk",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "road",
      "filter": ["all", ["==", ["get", "structure"], "bridge"], ["match", ["get", "class"], ["motorway", "trunk"], true, false]],
      "paint": {
        "line-color": "#8B7355",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          5, 0.5,
          14, 5,
          16, 10
        ]
      }
    },
    {
      "id": "aeroway-polygon",
      "type": "fill",
      "source": "mapbox-streets",
      "source-layer": "aeroway",
      "filter": ["==", ["geometry-type"], "Polygon"],
      "paint": {
        "fill-color": "#4A3B30",
        "fill-opacity": 0.6
      }
    },
    {
      "id": "aeroway-runway",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "aeroway",
      "filter": ["==", ["get", "type"], "runway"],
      "paint": {
        "line-color": "#5C4A3D",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          10, 1,
          14, 8
        ]
      }
    },
    {
      "id": "waterway",
      "type": "line",
      "source": "mapbox-streets",
      "source-layer": "waterway",
      "paint": {
        "line-color": "#264653",
        "line-width": [
          "interpolate", ["linear"], ["zoom"],
          8, 0.5,
          14, 2
        ]
      }
    },
    {
      "id": "water-label",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "natural_label",
      "filter": ["==", ["get", "class"], "water"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Italic", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          8, 10,
          14, 14
        ],
        "text-max-width": 8
      },
      "paint": {
        "text-color": "#5C8A9A",
        "text-halo-color": "#264653",
        "text-halo-width": 1
      }
    },
    {
      "id": "poi-label",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "poi_label",
      "minzoom": 14,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": 11,
        "text-max-width": 8,
        "text-anchor": "top",
        "text-offset": [0, 0.5]
      },
      "paint": {
        "text-color": "#A89080",
        "text-halo-color": "#2A1E17",
        "text-halo-width": 1
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "road",
      "minzoom": 12,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          12, 10,
          16, 13
        ],
        "symbol-placement": "line",
        "text-max-angle": 30
      },
      "paint": {
        "text-color": "#A89080",
        "text-halo-color": "#3D2E24",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "place-neighborhood",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "place_label",
      "filter": ["==", ["get", "class"], "neighbourhood"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          12, 11,
          16, 14
        ],
        "text-max-width": 8,
        "text-transform": "uppercase",
        "text-letter-spacing": 0.1
      },
      "paint": {
        "text-color": "#8B7B6B",
        "text-halo-color": "#3D2E24",
        "text-halo-width": 1
      }
    },
    {
      "id": "place-town",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "place_label",
      "filter": ["==", ["get", "class"], "town"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          8, 11,
          14, 15
        ],
        "text-max-width": 10
      },
      "paint": {
        "text-color": "#B8A898",
        "text-halo-color": "#3D2E24",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "place-city",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "place_label",
      "filter": ["==", ["get", "class"], "city"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          4, 12,
          10, 20
        ],
        "text-max-width": 10
      },
      "paint": {
        "text-color": "#D4C4B4",
        "text-halo-color": "#3D2E24",
        "text-halo-width": 2
      }
    },
    {
      "id": "country-label",
      "type": "symbol",
      "source": "mapbox-streets",
      "source-layer": "place_label",
      "filter": ["==", ["get", "class"], "country"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate", ["linear"], ["zoom"],
          2, 10,
          6, 18
        ],
        "text-max-width": 10
      },
      "paint": {
        "text-color": "#D4C4B4",
        "text-halo-color": "#3D2E24",
        "text-halo-width": 2
      }
    }
  ]
}
