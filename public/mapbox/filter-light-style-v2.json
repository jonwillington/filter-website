{
  "version": 8,
  "name": "Filter Light v2",
  "sources": {
    "composite": {
      "url": "mapbox://mapbox.mapbox-streets-v8",
      "type": "vector"
    }
  },
  "glyphs": "mapbox://fonts/mapbox/{fontstack}/{range}.pbf",
  "sprite": "mapbox://sprites/mapbox/streets-v12",
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#FAF7F4"
      }
    },
    {
      "id": "water",
      "type": "fill",
      "source": "composite",
      "source-layer": "water",
      "paint": {
        "fill-color": "#E8E0D8"
      }
    },
    {
      "id": "landuse-park",
      "type": "fill",
      "source": "composite",
      "source-layer": "landuse",
      "filter": ["==", ["get", "class"], "park"],
      "paint": {
        "fill-color": "#E5E8DF",
        "fill-opacity": ["interpolate", ["linear"], ["zoom"], 10, 0, 12, 0.5, 16, 0.7]
      }
    },
    {
      "id": "road-motorway",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 5,
      "filter": ["==", ["get", "class"], "motorway"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#D5CAC0",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          5, 0.75,
          10, 2,
          14, 5,
          18, 12
        ]
      }
    },
    {
      "id": "road-trunk",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 6,
      "filter": ["==", ["get", "class"], "trunk"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#DDD4CC",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          6, 0.5,
          10, 1.5,
          14, 4,
          18, 10
        ]
      }
    },
    {
      "id": "road-primary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 8,
      "filter": ["==", ["get", "class"], "primary"],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#E5DDD5",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          8, 0.5,
          12, 1.5,
          14, 3,
          18, 8
        ]
      }
    },
    {
      "id": "road-secondary-tertiary",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 10,
      "filter": [
        "any",
        ["==", ["get", "class"], "secondary"],
        ["==", ["get", "class"], "tertiary"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#EBE4DD",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          10, 0.3,
          14, 1.5,
          18, 6
        ]
      }
    },
    {
      "id": "road-street",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 13,
      "filter": [
        "any",
        ["==", ["get", "class"], "street"],
        ["==", ["get", "class"], "street_limited"]
      ],
      "layout": {
        "line-cap": "round",
        "line-join": "round"
      },
      "paint": {
        "line-color": "#F0EAE4",
        "line-width": [
          "interpolate",
          ["linear"],
          ["zoom"],
          13, 0.5,
          16, 2,
          18, 5
        ]
      }
    },
    {
      "id": "road-rail",
      "type": "line",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 10,
      "filter": ["==", ["get", "class"], "major_rail"],
      "paint": {
        "line-color": "#D8CFC5",
        "line-width": 1,
        "line-dasharray": [3, 3]
      }
    },
    {
      "id": "building",
      "type": "fill",
      "source": "composite",
      "source-layer": "building",
      "minzoom": 14,
      "paint": {
        "fill-color": "#EDE6E0",
        "fill-opacity": [
          "interpolate",
          ["linear"],
          ["zoom"],
          14, 0,
          16, 0.5
        ]
      }
    },
    {
      "id": "admin-country-boundary",
      "type": "line",
      "source": "composite",
      "source-layer": "admin",
      "filter": [
        "all",
        ["==", ["get", "admin_level"], 0],
        ["==", ["get", "disputed"], "false"]
      ],
      "paint": {
        "line-color": "#C4B5A5",
        "line-width": 1,
        "line-opacity": 0.5
      }
    },
    {
      "id": "road-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "road",
      "minzoom": 14,
      "filter": [
        "all",
        ["has", "name"],
        ["in", ["get", "class"], ["literal", ["street", "street_limited", "primary", "secondary", "tertiary"]]]
      ],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": ["interpolate", ["linear"], ["zoom"], 14, 10, 18, 13],
        "symbol-placement": "line",
        "text-max-angle": 30,
        "text-padding": 1
      },
      "paint": {
        "text-color": "#8B7355",
        "text-halo-color": "#FAF7F4",
        "text-halo-width": 1.5,
        "text-opacity": ["interpolate", ["linear"], ["zoom"], 14, 0, 15, 1]
      }
    },
    {
      "id": "transit-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "transit_stop_label",
      "minzoom": 14,
      "filter": ["in", ["get", "mode"], ["literal", ["rail", "metro_rail", "light_rail"]]],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": 11,
        "text-anchor": "top",
        "text-offset": [0, 0.6],
        "icon-image": "rail",
        "icon-size": 0.9,
        "icon-allow-overlap": false
      },
      "paint": {
        "text-color": "#6B5235",
        "text-halo-color": "#FAF7F4",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "poi-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "poi_label",
      "minzoom": 15,
      "filter": ["in", ["get", "class"], ["literal", ["food_and_drink", "shop", "landmark", "park_like"]]],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": 11,
        "text-anchor": "top",
        "text-offset": [0, 0.5],
        "icon-image": ["get", "maki"],
        "icon-size": 0.8,
        "icon-allow-overlap": false,
        "text-optional": true
      },
      "paint": {
        "text-color": "#6B5235",
        "text-halo-color": "#FAF7F4",
        "text-halo-width": 1,
        "text-opacity": ["interpolate", ["linear"], ["zoom"], 15, 0, 16, 1]
      }
    },
    {
      "id": "place-label-neighborhood",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "filter": [
        "any",
        ["==", ["get", "class"], "neighbourhood"],
        ["==", ["get", "class"], "suburb"]
      ],
      "minzoom": 12,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": 11,
        "text-transform": "uppercase",
        "text-letter-spacing": 0.1
      },
      "paint": {
        "text-color": "#A89080",
        "text-halo-color": "#FAF7F4",
        "text-halo-width": 1
      }
    },
    {
      "id": "place-label-town",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "filter": ["==", ["get", "class"], "town"],
      "minzoom": 8,
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Regular", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          8, 10,
          12, 13
        ]
      },
      "paint": {
        "text-color": "#8B7355",
        "text-halo-color": "#FAF7F4",
        "text-halo-width": 1
      }
    },
    {
      "id": "place-label-city",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "filter": ["==", ["get", "class"], "city"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          4, 10,
          10, 16
        ]
      },
      "paint": {
        "text-color": "#6B5235",
        "text-halo-color": "#FAF7F4",
        "text-halo-width": 1.5
      }
    },
    {
      "id": "country-label",
      "type": "symbol",
      "source": "composite",
      "source-layer": "place_label",
      "filter": ["==", ["get", "class"], "country"],
      "layout": {
        "text-field": ["get", "name"],
        "text-font": ["DIN Pro Medium", "Arial Unicode MS Regular"],
        "text-size": [
          "interpolate",
          ["linear"],
          ["zoom"],
          2, 10,
          6, 14
        ],
        "text-transform": "uppercase",
        "text-letter-spacing": 0.1
      },
      "paint": {
        "text-color": "#8B6F47",
        "text-halo-color": "#FAF7F4",
        "text-halo-width": 2
      }
    }
  ]
}
